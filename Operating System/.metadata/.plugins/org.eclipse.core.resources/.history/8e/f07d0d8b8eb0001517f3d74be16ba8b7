		 stringToLong("hello");
	}

	private static long stringToLong(String string) {
		StringBuffer sb = new StringBuffer();
		for(int i = 0; i < string.length(); ++i)
		{
		  int ch = (int)string.charAt(i);
		}
		return 0;
	}
}
