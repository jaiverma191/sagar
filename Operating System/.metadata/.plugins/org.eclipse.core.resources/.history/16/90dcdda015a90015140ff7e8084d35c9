package com.hiring.tree;

public class Test {

	public static void main(String[] args) {
		int nonWord = 0;
		int count = 0;
		String regex = "[a-zA-Z].*";

		while (scanner.hasNext()) {
			String word = scanner.next();
			if (word.matches(regex)) {
				count++;
			} else {
				nonWord++;
			}
		}
		return count;
	}
}
